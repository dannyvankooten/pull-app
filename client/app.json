{
  "expo": {
    "name": "Pull-up Game",
    "slug": "pullup-game",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#2185d0",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      	"supportsTablet": true,
		"bundleIdentifier": "com.dvko.pullupgame"
    },
	"android": {
      "package": "com.dvko.pullupgame",
      "permissions": []
	},
    "description": "Compete against your friends doing pull-ups",
    "githubUrl": "https://github.com/dannyvankooten/pull-app"
  }
}
